    "num_sampling_steps": {
      "type": "integer",
      "description": "Num Sampling Steps"
    },
    "cfg_scale": {
      "type": "number",
      "description": "CFG Scale"
    },
    "solver": {
      "type": "string",
      "widget": "select",
      "description": "Solver",
      "oneOf": [
        {
          "description": "euler",
          "enum": [
            "euler"
          ]
        },
        {
          "description": "midpoint",
          "enum": [
            "midpoint"
          ]
        },
        {
          "description": "rk4",
          "enum": [
            "rk4"
          ]
        }
      ],
      "default": "midpoint"
    },
    "t_shift": {
      "type": "number",
      "description": "Time Shift"
    },
    "seed": {
      "type": "number",
      "description": "Seed (0 = random)"
    },
    "scaling_method": {
      "type": "string",
      "widget": "select",
      "description": "Scaling Method",
      "oneOf": [
        {
          "description": "Time-aware",
          "enum": [
            "Time-aware"
          ]
        },
        {
          "description": "None",
          "enum": [
            "None"
          ]
        }
      ],
      "default": "Time-aware"
    },
