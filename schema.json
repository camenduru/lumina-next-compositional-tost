        {
          "description": "1024x2048 (1x4 Grids)",
          "enum": [
            "1024x2048 (1x4 Grids)"
          ]
        },
        {
          "description": "1024x2560 (1x4 Grids)",
          "enum": [
            "1024x2560 (1x4 Grids)"
          ]
        },
        {
          "description": "1024x3072 (1x4 Grids)",
          "enum": [
            "1024x3072 (1x4 Grids)"
          ]
        }
      ],
      "default": "2048x1024 (4x1 Grids)"
